class Person:
  def __init__(self, fname, lname):
    self.firstname = fname
    self.lastname = lname

  def printname(self):
    print(self.firstname, self.lastname)

class Student(Person):
  pass

x = Student("jovitta ", "george eluvathingal ")
y = Student("milu","george")
z = Student("halena","jofin")
x.printname()
y.printname()
z.printname()
